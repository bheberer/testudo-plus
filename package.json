{
  "name": "testudo-plus",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.3.0",
    "@material-ui/icons": "^1.1.0",
    "axios": "^0.18.0",
    "bootstrap": "^3.3.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "jest": "^23.4.2",
    "jquery": "^3.3.1",
    "react": "^16.4.1",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "recompose": "^0.28.2",
    "redux": "^4.0.0",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "typeface-roboto": "0.0.54"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "jest --no-cache",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-preset-env": "^1.7.0",
    "mocha": "^5.2.0",
    "react-test-renderer": "^16.4.1",
    "reactotron-react-js": "^2.0.0",
    "reactotron-redux": "^2.0.0",
    "redux-devtools-extension": "^2.13.5",
    "regenerator-runtime": "^0.12.0",
    "webdriverio": "^4.13.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>src/Tests/Setup/setup-enzyme.js",
    "testURL": "http://localhost",
    "testPathIgnorePatterns": [
      "<rootDir>src/Tests/Setup/"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>src/Test/Jest/file-mocks.js",
      "\\.(css|less)$": "<rootDir>src/Tests/Jest//style-mock.js"
    }
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  }
}
